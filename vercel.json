{
    "version": 2,
    "builds": [
        {
            "src": "build.sh",
            "use": "@vercel/python",
            "config": {
                "distDir": "public"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}